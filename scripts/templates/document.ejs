<header role="navigation">
  <small>
    <ul>
      <% document.breadcrumbs.map((breadcrumb) => { %>
      <li>
        <a href="/<%- breadcrumb.filename %>.html" hx-get="/<%- breadcrumb.filename %>.html" hx-swap="show:top"
          hx-target="#main" hx-push-url="/<%- breadcrumb.filename %>"><%- breadcrumb.title %></a>
      </li>
      <% }) %>
    </ul>
    <span class="meta">
      <a href="https://github.com/parksb/pedia/commits/master/docs/<%- document.filename %>.md">history</a> Â·
      <a href="https://raw.githubusercontent.com/parksb/pedia/master/docs/<%- document.filename %>.md">raw</a>
    </span>
  </small>
</header>
<article><%- document.html %></article>
<% if (document.referredFrom.length > 0) { %>
  <section class="backlinks">
    <small>
      Backlinks:
      <ul>
        <% document.referredFrom.map((document) => { %>
        <li>
          <a href="/<%- document.filename %>.html" hx-get="/<%- document.filename %>.html" hx-swap="show:top"
            hx-target="#main" hx-push-url="/<%- document.filename %>"><%- document.title %></a>
        </li>
        <% }) %>
      </ul>
    </small>
  </section>
<% } %>
